<!-- name of layout -->
{{!< ../adminLayout}} 
<div class="content-box-large">
	<div class="panel-heading">
		<div class="panel-title">Users</div>
	</div>
	<div class="panel-body">
		<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
			<thead>
				<tr>
					<th>Name</th>
					<th>Email</th>
					<th>Log</th>
					<th>Details</th>
					<th>Edit</th>
				</tr>
			</thead>
			<tbody>

				{{#each Users}}
				<tr class="odd gradeX">
					<td>{{name}}</td>
					<td>{{email}}</td>
					<td>Log</td>
					<td class="center">
						<a class="view-icon" href="#"><i class="glyphicon glyphicon-eye-open"></i></a>
					</td>
					<td class="center">
						<a class="edit-icon" href="#"><i class="glyphicon glyphicon-pencil"></i></a>
					</td>
				</tr>
				{{/each}}
				</tr>
			</tbody>
		</table>
	</div>
	</div>